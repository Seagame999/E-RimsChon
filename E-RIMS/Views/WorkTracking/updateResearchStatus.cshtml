                                    <label class="">วัตถุประสงค์</label>
                                    <label class="form-control" name="objective">@Model.objective</label>
                                    <input type="hidden" name="objective" value="@Model.objective">
                                </div>
                                <div class="form-group col-3">
                                    <label class="">ประโยชน์ที่คาดว่าจะได้รับ</label>
                                    <label class="form-control" name="benefit">@Model.benefit</label>
                                    <input type="hidden" name="benefit" value="@Model.benefit">
                                </div>
