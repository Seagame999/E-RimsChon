    {
        <div class="card-body">
            <h5 class="card-title"><i class="fa fa-project-diagram"></i>&nbsp;การกำกับติดตามงานวิจัย</h5>
            <hr />
            <div class="row">
                <div class="col-12">
                    <div class="card">
                        <div class="card-header">
                            <h6>
                                <i class="fa fa-user-circle"></i>&nbsp;ผู้ดำเนินงานวิจัย และข้อมูลงานวิจัย&nbsp;
                                @if (Session["Role"] != null)
                                {
                                    if (Session["Role"].Equals("Admin"))
                                    {
                                        <a href="@Url.Action("EditResearch"+"/"+Model.id, "Research")" class="btn btn-sm btn-info"><i class="fa fa-edit"></i>&nbsp;แก้ไขข้อมูลงานวิจัย</a>
                                    }
                                }

                            </h6>
                        </div>
                        <div class="card-body">
                            <div class="form-row">
                                <div class="form-group col-2">
                                    <label class="">ประเภทงาน</label>
                                    <label class="form-control" name="type">@Model.type</label>
                                    <input type="hidden" name="type" value="@Model.type">
                                </div>
                                <div class="form-group col-2">
                                    <label class="">ปีงบประมาณ</label>
                                    <label class="form-control" name="budgetYear">@Model.budgetYear</label>
                                    <input type="hidden" name="budgetYear" value="@Model.budgetYear">
                                </div>
                                <div class="form-group col-8">
                                    <label class="">ชื่อเรื่อง</label>
                                    <label class="form-control" name="name">@Model.name</label>
                                    <input type="hidden" name="name" value="@Model.name">
                                </div>
                                <div class="form-group col-6">
                                    <label class="">ผู้จัดทำ</label>
                                    <label class="form-control" name="creator">@Model.creator</label>
                                    <input type="hidden" name="creator" value="@Model.creator">
                                </div>
                                <div class="form-group col-6">
                                    <label class="">กลุ่มงาน</label>
                                    <label class="form-control" name="workGroup">@Model.workGroup</label>
                                    <input type="hidden" name="workGroup" value="@Model.workGroup">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <br />
